<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="505.0" prefWidth="670.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.project4.StoreOrdersController">
   <children>
      <Label layoutX="24.0" layoutY="32.0" prefHeight="34.0" prefWidth="198.0" text="Customer Phone Number">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <ComboBox fx:id="phoneNumbers" layoutX="222.0" layoutY="37.0" onAction="#changeNumber" prefWidth="150.0" />
      <Label layoutX="391.0" layoutY="33.0" prefHeight="34.0" prefWidth="106.0" text="Order Total $">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <TextField fx:id="orderTotal" editable="false" layoutX="497.0" layoutY="37.0" />
      <ListView fx:id="ordersView" editable="true" layoutX="23.0" layoutY="74.0" prefHeight="307.0" prefWidth="619.0" />
      <Button fx:id="cancelOrderButton" layoutX="123.0" layoutY="411.0" mnemonicParsing="false" onAction="#cancelOrder" prefHeight="34.0" prefWidth="149.0" text="Cancel Order">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="exportStoreOrdersButton" layoutX="333.0" layoutY="411.0" mnemonicParsing="false" onAction="#exportStoreOrders" prefHeight="39.0" prefWidth="182.0" text="Export Store Orders">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
